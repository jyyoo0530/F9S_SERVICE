GET         /assets/*file                                                controllers.Assets.versioned(path="/public", file: Asset)
GET         /                                                            controllers.HomeController.index
GET         /wstest                                                      controllers.SocketController.msgRoom
GET         /ws                                                          controllers.SocketController.msg
GET         /stomptest                                                   controllers.SocketController.msgRoom




